PORTB = $6000
PORTA = $6001
DDRB = $6002
DDRA = $6003

E  = %10000000
RW = %01000000
RS = %00100000

  .org $0200

reset:
  lda #%11111111 ; Set all pins on port B to output
  sta DDRB
  jsr lcd_instruction
  jmp reset

lcd_instruction:
  lda #0         ; Clear RS/RW/E bits
  sta PORTA
  lda #E         ; Set E bit to send instruction
  sta PORTA
  lda #0         ; Clear RS/RW/E bits
  sta PORTA
  rts





 